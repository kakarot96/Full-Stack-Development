<% include partials/header %>

<% blogs.forEach(function(blog){ %>
    <div>
        <h1><%=blog.title %></h1>
        <img src="<%=blog.image%>">
        <p><%=blog.body.substring(0,100)%>...</p>
        <p><%=blog.created%></p>
        <a href="/blogs/<%=blog._id%>">Show More</a>
    </div>
<% }) %>
<% include partials/footer %>